arr = [0]*7
for i in range(0,7):
    ele = int(input("Enter"))
    arr.append(ele)

largest = arr[0]
sec_largest = arr[1]
for  i in range(1,len(arr)):
    if arr[i] > largest:
        sec_largest = largest
        largest = arr[i] 
    
    elif arr[i]<largest and arr[i]> sec_largest:
        sec_largest = arr[i]
print(sec_largest)

#big o of n and works for all even duplicates in arrray